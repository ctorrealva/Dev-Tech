<p id="notice"><%= notice %></p>

<h1>Listing Administradors</h1>

<table>
  <thead>
    <tr>
      <th>Dni</th>
      <th>User</th>
      <th>Nombres</th>
      <th>Apellidopaterno</th>
      <th>Apellidomaterno</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @administradors.each do |administrador| %>
      <tr>
        <td><%= administrador.DNI %></td>
        <td><%= administrador.user %></td>
        <td><%= administrador.Nombres %></td>
        <td><%= administrador.ApellidoPaterno %></td>
        <td><%= administrador.ApellidoMaterno %></td>
        <td><%= link_to 'Show', administrador %></td>
        <td><%= link_to 'Edit', edit_administrador_path(administrador) %></td>
        <td><%= link_to 'Destroy', administrador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

