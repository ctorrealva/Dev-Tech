<%= simple_form_for(@favorite) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :client %>
    <%= f.input :source %>
    <%= f.input :destination %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
